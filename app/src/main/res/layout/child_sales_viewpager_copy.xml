<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:gravity="top"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:orientation="horizontal"
        android:weightSum="2">

        <RelativeLayout
            android:id="@+id/netsales"
            android:layout_width="0dp"
            android:layout_height="80dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:background="@drawable/salescard">



            <TextView
                android:id="@+id/txtNetSales"
                android:layout_width="wrap_content"
                android:layout_height="16dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="5dp"
                android:gravity="top"
                android:layout_marginTop="2dp"
                android:text="Net Sales"
                android:textSize="12sp" />


            <ImageView
                android:id="@+id/txtNetSalesImage"
                android:layout_width="19dp"
                android:layout_height="16dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:background="@android:color/holo_green_dark" />

            <TextView
                android:id="@+id/txtNetSalesVal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/txtNetSalesImage"
                android:layout_marginBottom="15dp"
                android:layout_marginRight="8dp"
                android:layout_marginTop="15dp"
                android:gravity="right"
                android:text="12,15,1215"
                android:textColor="@android:color/black"
                android:textSize="16sp" />


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="2dp"
                android:layout_marginTop="10dp"
                android:layout_alignParentBottom="true"
                android:layout_below="@+id/txtNetSales">

                <TextView
                    android:id="@+id/txtNetSalesName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_marginRight="8dp"
                    android:layout_toLeftOf="@+id/txtNetSalesPerc"
                    android:text="djshjd"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/txtNetSalesPerc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="8dp"
                    android:text="20%"
                    android:textColor="@android:color/black"
                    android:textSize="11sp" />

            </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/plansales"
            android:layout_width="0dp"
            android:layout_height="80dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:background="@drawable/salescard">


            <TextView
                android:id="@+id/txtPlanSales"
                android:layout_width="wrap_content"
                android:layout_height="16dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="5dp"
                android:gravity="top"
                android:layout_marginTop="2dp"
                android:text="Plan Sales"
                android:textSize="12sp" />


            <ImageView
                android:id="@+id/txtPlanSalesImage"
                android:layout_width="19dp"
                android:layout_height="16dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:background="@android:color/holo_green_dark" />

            <TextView
                android:id="@+id/txtPlanSalesVal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/txtPlanSalesImage"
                android:layout_marginBottom="15dp"
                android:layout_marginRight="8dp"
                android:layout_marginTop="15dp"
                android:gravity="right"
                android:text="12,15,1215"
                android:textColor="@android:color/black"
                android:textSize="16sp" />

            <RelativeLayout
                android:layout_marginBottom="2dp"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_below="@+id/txtPlanSales">

                <TextView
                    android:id="@+id/txtPlanSalesName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_marginRight="8dp"
                    android:layout_toLeftOf="@+id/txtPlanSalesPerc"
                    android:text="djshjd"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/txtPlanSalesPerc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="8dp"
                    android:text="20%"
                    android:textColor="@android:color/black"
                    android:textSize="11sp" />


            </RelativeLayout>
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:orientation="horizontal"
        android:weightSum="2">

        <RelativeLayout
            android:id="@+id/netsalesu"
            android:layout_width="0dp"
            android:layout_height="80dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:background="@drawable/salescard">

            <TextView
                android:id="@+id/txtNetSalesU"
                android:layout_width="wrap_content"
                android:layout_height="16dp"
                android:layout_alignParentLeft="true"
                android:gravity="top"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="2dp"
                android:text="Net Sales(U)"
                android:textSize="12sp" />


            <ImageView
                android:id="@+id/txtNetSalesUImage"
                android:layout_width="19dp"
                android:layout_height="16dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:background="@android:color/holo_green_dark" />

            <TextView
                android:id="@+id/txtNetSalesUVal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/txtNetSalesUImage"
                android:layout_marginBottom="15dp"
                android:layout_marginRight="8dp"
                android:layout_marginTop="15dp"
                android:gravity="right"
                android:text="12,15,1215"
                android:textColor="@android:color/black"
                android:textSize="16sp" />


            <RelativeLayout
                android:layout_marginBottom="2dp"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_below="@+id/txtNetSalesU">

                <TextView
                    android:id="@+id/txtNetSalesUName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_marginRight="8dp"
                    android:layout_toLeftOf="@+id/txtNetSalesUPerc"
                    android:text="djshjd"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/txtNetSalesUPerc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="8dp"
                    android:text="20%"
                    android:textColor="@android:color/black"
                    android:textSize="11sp" />


            </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/sohu"
            android:layout_width="0dp"
            android:layout_height="80dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:background="@drawable/salescard">

            <TextView
                android:id="@+id/txtSoHU"
                android:layout_width="wrap_content"
                android:layout_height="16dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="5dp"
                android:gravity="top"
                android:layout_marginTop="2dp"
                android:text="SOH(U)"
                android:textSize="12sp" />


            <ImageView
                android:id="@+id/txtSoHUImage"
                android:layout_width="19dp"
                android:layout_height="16dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp" />


            <TextView
                android:id="@+id/txtSoHUVal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/txtSoHUImage"
                android:layout_marginBottom="15dp"
                android:layout_marginRight="8dp"
                android:layout_marginTop="15dp"
                android:gravity="right"
                android:text="12,15,1215"
                android:textColor="@android:color/black"
                android:textSize="16sp" />


        </RelativeLayout>

    </LinearLayout>

