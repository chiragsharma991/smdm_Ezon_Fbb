<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="fill_parent"
              android:layout_height="fill_parent"
              android:background="@color/bg_screen_color"
              android:gravity="top"
              android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/RelLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="10dp">

    <RelativeLayout
            android:id="@+id/sznRelLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/sznchildRelLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible">

                <RelativeLayout
                    android:id="@+id/rel_ez_storesales"
                    android:layout_width="90dp"
                    android:layout_height="100dp"
                    android:layout_alignParentStart="true"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/rounded_edt"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txt_ez_PvASales"
                        android:layout_width="match_parent"
                        android:layout_height="20dp"
                        android:layout_margin="6dp"
                        android:gravity="center"
                        android:text="STORE"
                        android:textColor="@android:color/white"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/txt_ez_StoreVal_PvASales1"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_below="@+id/txt_ez_PvASales"
                        android:background="@color/edit_border_color"
                        android:gravity="center"
                        android:text="40%"
                        android:textColor="@android:color/black"
                        android:textSize="18sp" />


                    <TextView
                        android:id="@+id/txt_ez_StoreVal_PvASalesName"
                        android:layout_width="match_parent"
                        android:layout_height="20dp"
                        android:layout_below="@+id/txt_ez_StoreVal_PvASales1"
                        android:gravity="center" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rel_ez_zonalsales"
                    android:layout_width="90dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="35dp"
                    android:layout_toRightOf="@+id/rel_ez_storesales"
                    android:background="@drawable/rounded_edt"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txt_ez_ZonalSales"
                        android:layout_width="match_parent"
                        android:layout_height="20dp"
                        android:layout_margin="6dp"
                        android:gravity="center"
                        android:text="@string/zonal"
                        android:textColor="@android:color/white"
                        android:textSize="14sp" />


                    <TextView
                        android:id="@+id/txt_ez_ZonalName"
                        android:layout_width="match_parent"
                        android:layout_height="20dp"
                        android:layout_below="@+id/txt_ez_ZonalSalesVal"
                        android:gravity="center" />

                    <TextView
                        android:id="@+id/txt_ez_ZonalSalesVal"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/txt_ez_ZonalSales"
                        android:background="@color/edit_border_color"
                        android:gravity="center"
                        android:text="40%"
                        android:textColor="@android:color/black"
                        android:textSize="18sp" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rel_ez_nationalsales"
                    android:layout_width="90dp"
                    android:layout_height="100dp"
                    android:layout_alignParentEnd="true"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/rounded_edt"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txt_ez_NationalSales"
                        android:layout_width="match_parent"
                        android:layout_height="20dp"
                        android:layout_margin="6dp"
                        android:gravity="center"
                        android:text="@string/national"
                        android:textColor="@android:color/white"
                        android:textSize="14sp" />



                    <TextView
                        android:id="@+id/txt_ez_NationalSalesVal"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_below="@+id/txt_ez_NationalSales"
                        android:background="@color/edit_border_color"
                        android:gravity="center"
                        android:text="40%"
                        android:textColor="@android:color/black"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/txt_ez_NationalName"
                        android:layout_width="match_parent"
                        android:layout_height="20dp"
                        android:layout_below="@+id/txt_ez_NationalSalesVal"
                        android:gravity="center" />

                </RelativeLayout>
            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/rankRelLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone">

                <RelativeLayout
                    android:id="@+id/rel_ez_pvaSales"
                    android:layout_width="160dp"
                    android:layout_height="100dp"
                    android:layout_alignParentStart="true"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/rounded_edittext5"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/znLayout"
                        android:layout_width="match_parent"
                        android:layout_height="20dp"
                        android:layout_margin="3dp"
                        android:orientation="horizontal"
                        android:weightSum="2">

                        <TextView
                            android:id="@+id/txt_ez_PvAZonalRank"
                            android:layout_width="match_parent"
                            android:layout_height="20dp"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="@string/zonal"
                            android:textColor="@color/bg_store_code_text"
                            android:textSize="14sp"/>

                        <TextView
                            android:id="@+id/txt_ez_PvANationalRank"
                            android:layout_width="match_parent"
                            android:layout_height="20dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="@string/national"
                            android:textColor="@color/bg_store_code_text"
                            android:textSize="14sp"/>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/pvaValLayout"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_below="@+id/znLayout"
                        android:orientation="horizontal"
                        android:weightSum="3">

                        <LinearLayout
                            android:id="@+id/lin_ez_PvAZonalRank"
                            android:layout_width="79dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <TextView
                                android:id="@+id/txt_ez_PvAZonalRankVal"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="1"
                                android:textColor="@android:color/black"
                                android:textSize="18sp"/>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="2dip"
                            android:layout_height="50dp"
                            android:layout_weight="1">

                            <View
                                android:id="@+id/txtPvAView"
                                android:layout_width="2dip"
                                android:layout_height="match_parent"
                                android:layout_marginTop="0dp"
                                android:background="@color/colorPrimary"/>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/lin_ez_PvANationalRank"
                            android:layout_width="79dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <TextView

                                android:id="@+id/txt_ez_PvANationalRankVal"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="11"
                                android:textColor="@android:color/black"
                                android:textSize="18sp"/>

                        </LinearLayout>
                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:layout_below="@+id/pvaValLayout"
                        android:layout_margin="2dp"
                        android:gravity="center"
                        android:text="@string/sales_title"
                        android:textColor="@color/bg_store_code_text"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rel_ez_yoySales"
                    android:layout_width="160dp"
                    android:layout_height="100dp"
                    android:layout_marginLeft="20dp"
                    android:layout_toEndOf="@+id/rel_ez_pvaSales"
                    android:background="@drawable/rounded_edittext5"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/yoyznLayout"
                        android:layout_width="match_parent"
                        android:layout_height="20dp"
                        android:layout_margin="3dp"
                        android:orientation="horizontal"
                        android:weightSum="2">

                        <TextView
                            android:id="@+id/txt_ez_YoYZonalRank"
                            android:layout_width="match_parent"
                            android:layout_height="20dp"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="@string/zonal"
                            android:textColor="@color/bg_store_code_text"
                            android:textSize="14sp"/>

                        <TextView
                            android:id="@+id/txt_ez_YoYNationalRank"
                            android:layout_width="match_parent"
                            android:layout_height="20dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="@string/national"
                            android:textColor="@color/bg_store_code_text"
                            android:textSize="14sp"/>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/yoyValLayout"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_below="@+id/yoyznLayout"
                        android:orientation="horizontal"
                        android:weightSum="3">

                        <LinearLayout
                            android:id="@+id/lin_ez_YoYZonalRank"
                            android:layout_width="79dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <TextView
                                android:id="@+id/txt_ez_YoYZonalRankVal"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="11"
                                android:textColor="@android:color/black"
                                android:textSize="18sp"/>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="2dip"
                            android:layout_height="50dp"
                            android:layout_weight="1">

                            <View
                                android:id="@+id/txtYoYView"
                                android:layout_width="2dip"
                                android:layout_height="match_parent"
                                android:layout_marginTop="0dp"
                                android:background="@color/colorPrimary"/>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/lin_ez_YoYNationalRank"
                            android:layout_width="79dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">


                            <TextView
                                android:id="@+id/txt_ez_YoYNationalRankVal"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="11"
                                android:textColor="@android:color/black"
                                android:textSize="18sp"/>

                        </LinearLayout>


                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:layout_below="@+id/yoyValLayout"
                        android:layout_margin="2dp"
                        android:gravity="center"
                        android:text="@string/yoy_sales_grw"
                        android:textColor="@color/bg_store_code_text"/>
                </RelativeLayout>

            </RelativeLayout>
        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/pmsyrtxtRelLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/sznRelLayout"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp">

            <TextView
                android:id="@+id/txt_ez_PvA"
                android:layout_width="65dp"
                android:layout_height="30dp"
                android:background="@drawable/rounded_edittext3"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center"
                android:text="@string/pva"
                android:textColor="#000000"
                android:textSize="9sp"/>


            <TextView
                android:id="@+id/txt_ez_YoY"
                android:layout_width="65dp"
                android:layout_height="30dp"
                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@+id/txt_ez_PvA"
                android:background="@drawable/rounded_edittext3"
                android:clickable="true"
                android:gravity="center"
                android:text="@string/yoy"
                android:textColor="#000000"
                android:textSize="9sp"/>


            <TextView
                android:id="@+id/txt_ez_SellThru"
                android:layout_width="65dp"
                android:layout_height="30dp"
                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@+id/txt_ez_YoY"
                android:background="@drawable/rounded_edittext3"
                android:clickable="true"
                android:gravity="center"
                android:text="@string/sell_thru"
                android:textColor="#000000"
                android:textSize="9sp"/>


            <TextView
                android:id="@+id/txt_ez_Mixsales"
                android:layout_width="65dp"
                android:layout_height="30dp"
                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@+id/txt_ez_SellThru"
                android:background="@drawable/rounded_edittext3"
                android:clickable="true"
                android:gravity="center"
                android:text="@string/mixsales"
                android:textColor="#000000"
                android:textSize="9sp"/>


            <TextView
                android:id="@+id/txt_ez_rank"
                android:layout_width="65dp"
                android:layout_height="30dp"
                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@+id/txt_ez_Mixsales"
                android:background="@drawable/rounded_edittext3"
                android:clickable="true"
                android:gravity="center"
                android:text="@string/rank"
                android:textColor="#000000"
                android:textSize="9sp"/>
        </RelativeLayout>
    </RelativeLayout>
</LinearLayout>