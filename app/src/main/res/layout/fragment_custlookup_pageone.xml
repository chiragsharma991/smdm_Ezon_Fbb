                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/smdm_divider_Black" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="110dp"
                    android:background="@color/white"
                    android:weightSum="2">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="4dp"
                        android:layout_weight="1">

                        <ProgressBar
                            android:id="@+id/circularProgressbar_customer"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="250dp"
                            android:layout_height="250dp"
                            android:indeterminate="false"
                            android:max="100"
                            android:progressDrawable="@drawable/circular"
                            android:secondaryProgress="100" />

                        <TextView
                            android:id="@+id/txt_progress_custVal"
                            android:layout_width="250dp"
                            android:layout_height="250dp"
                            android:layout_marginBottom="9dp"
                            android:gravity="center"
                            android:hint=""
                            android:textColor="@color/colorPrimaryDark"
                            android:textSize="19sp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/txt_progress_cust_nm"
                            android:layout_width="250dp"
                            android:layout_height="250dp"
                            android:gravity="center"
                            android:layout_marginBottom="5dp"
                            android:text="\n\nCustomer"
                            android:textColor="@color/smdm_default_color"
                            android:textSize="10sp" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <ProgressBar
                            android:id="@+id/circularProgressbar_sales"
                            style="?android:attr/progressBarStyleHorizontal"
                            android:layout_width="250dp"
                            android:layout_height="250dp"
                            android:indeterminate="false"
                            android:max="100"
                            android:progressDrawable="@drawable/circular"
                            android:secondaryProgress="100" />

                        <TextView
                            android:id="@+id/txt_progress_salesVal"
                            android:layout_width="250dp"
                            android:layout_height="250dp"
                            android:layout_marginBottom="9dp"
                            android:gravity="center"
                            android:hint=""
                            android:textColor="@color/colorPrimaryDark"
