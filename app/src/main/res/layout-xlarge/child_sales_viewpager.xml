                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="8dp"
                android:layout_marginTop="8dp"
                android:text="12,15,1215"
                android:textColor="@android:color/black"
                android:textSize="16sp"
                android:layout_alignBottom="@+id/txtPlanSales"
                android:layout_toEndOf="@+id/txtPlanSales" />

            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_below="@+id/txtPlanSales">

                <ImageView
                    android:background="@android:color/holo_green_dark"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginBottom="8dp"
                    android:layout_marginRight="8dp"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentRight="true"
                    android:id="@+id/txtPlanSalesImage" />

                <TextView
                    android:layout_toLeftOf="@+id/txtPlanSalesImage"
                    android:id="@+id/txtPlanSalesPerc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="8dp"
                    android:layout_marginBottom="8dp"
                    android:layout_alignParentBottom="true"
                    android:text="20%"
                    android:textColor="@android:color/black"
                    android:textSize="16sp" />

                <TextView
                    android:layout_toLeftOf="@+id/txtPlanSalesPerc"
                    android:id="@+id/txtPlanSalesName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_marginRight="8dp"
                    android:layout_marginBottom="8dp"
                    android:textSize="14sp" />

            </RelativeLayout>
        </RelativeLayout>
    </LinearLayout>

